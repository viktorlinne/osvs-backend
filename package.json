{
  "name": "osvs-backend",
  "version": "1.0.0",
  "description": "A simple Node + Express Typescript backend for a degree project for a fraternal organization to handle their members and website content",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "migrate": "ts-node src/db/migrate.ts",
    "cleanup:tokens": "ts-node src/scripts/cleanup_tokens.ts",
    "cron:tokens": "ts-node src/scripts/cron_runner.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viktorlinne/osvs-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/viktorlinne/osvs-backend/issues"
  },
  "homepage": "https://github.com/viktorlinne/osvs-backend#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.366.0",
    "@aws-sdk/s3-request-presigner": "^3.366.0",
    "@sentry/node": "^7.51.0",
    "@sentry/tracing": "^7.51.0",
    "argon2": "^0.30.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.15.3",
    "pino": "^8.16.0",
    "pino-http": "^8.0.0",
    "pino-pretty": "^11.0.1",
    "redis": "^4.6.7",
    "sharp": "^0.32.6",
    "sib-api-v3-sdk": "^8.5.0",
    "stripe": "^20.0.0",
    "zod": "^3.22.4",
    "@osvs/types": "file:../shared-types"
  },
  "devDependencies": {
    "@prisma/client": "^4.16.2",
    "@prisma/sdk": "^4.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.10.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "prisma": "^4.16.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
